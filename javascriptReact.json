{
  "React Component": {
    "prefix": "rc",
    "body": [
      "import React, { useState } from \"react\"",
      "",
      "function ${1:ComponentName}(props) {",
      "  return (",
      "    <>",
      "      $2",
      "    </>",
      "  )",
      "}",
      "",
      "export default ${1:ComponentName}"
    ],
    "description": "React Component"
  },
  "ArrowFunNamingExport": {
    "prefix": "fnae",
    "body": [
      "export const ${1:ComponentName} = (${2:parms}) => {",
      "    return ($3)",

      "}"
    ],
    "description": "Export Arrow Fun "
  },
  "ArrowFunContant": {
    "prefix": "fna",
    "body": ["const ${1:ComponentName} = (${2:parms}) => {$3}"],
    "description": "Arrow Fun  "
  },
  "styled components": {
    "prefix": "stc",
    "body": [
      "import  styled   from 'styled-components';",
      "export const $1 = styled.$2`",
      "$3",
      "      `;"
    ],
    "description": "Arrow Fun  "
  },
  "functionsNamed": {
    "prefix": "fn",
    "body": ["function ${1:ComponentName}(${2:parms}) {", "   $3 ", "}"],
    "description": "create new functions"
  },
  "stjsx": {
    "prefix": "style jsx",
    "body": [
      "<style jsx>",
      " {`",
      "${1:background-color}:${2:peruple};",
      "${3:property}:${4:value};",
      "$5",
      "`}",
      "</style>"
    ],
    "description": "functionsNamed"
  },
  "rest styles": {
    "prefix": "*::rest",
    "body": [
      "*,*::after,*::before{",
      "    box-sizing: border-box;",
      "    margin: 0;",
      "    padding: 0;",
      "}",
      "html{",
      "    font-size:62.5%;",
      "}",
      "",
      "body{",
      "    font-family: ${1:Arial, Helvetica, sans-serif};",
      "    background-color: #efefef;",
      "    -webkit-font-smoothing: antialiased;",
      "    -moz-osx-font-smoothing: grayscale;",
      "}",
      "img{",
      "     width:100%;",
      "     height:100%;",
      "     display:inline-block;",
      "     vertical-align: center;",
      "     object-fit: cover;",
      "}"
    ],
    "description": "rest styles"
  }
}
